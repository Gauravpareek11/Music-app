<h2 class="display-2">List of Requirements Posted</h2>
<div class="d-flex flex-wrap">
  <% @items.each do |item| %>
    <%= render partial: 'welcomes/item_card', locals: { item: item } %>
  <% end %>
  <%= paginate @items%>
</div>
<% if current_user %>
  <div id="sender" data-sender-id="<%= current_user.id %>"></div>
<% end %>
