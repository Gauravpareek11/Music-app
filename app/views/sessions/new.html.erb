<h1>Login</h1>

<%= form_tag '/login' do %>

  Email: <%= text_field_tag :email %>
  
  Password: <%= password_field_tag :password_digest %>
  Confirm Password: <%= password_field_tag :password_confirmation %>
  <%= submit_tag "Submit" %>

<% end %>
<%= link_to 'Sign in with Google', '/auth/google_oauth2',method: :post,authenticity_token: form_authenticity_token%>
